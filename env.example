# ServicesAI Environment Variables
# Copy this file to .env.local and update with your values

# OpenAI API Key (optional - for enhanced AI capabilities)
OPENAI_API_KEY=your_openai_api_key_here

# Application Settings
NEXT_PUBLIC_APP_NAME=ServicesAI
NEXT_PUBLIC_APP_VERSION=1.0.0

# Demo Mode Settings
NEXT_PUBLIC_DEMO_MODE=true
NEXT_PUBLIC_DEMO_USERS_ENABLED=true

# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000/api

# Feature Flags
NEXT_PUBLIC_ENABLE_VOICE_INTERFACE=false
NEXT_PUBLIC_ENABLE_MULTI_LANGUAGE=false
NEXT_PUBLIC_ENABLE_ADVANCED_ANALYTICS=false
